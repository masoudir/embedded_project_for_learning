{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug-STM32WL",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceRoot}/build/game01_nucleof446re.elf",
      "args": [],
      "stopAtEntry": true,
      "cwd": "${workspaceRoot}",
      "environment": [],
      "externalConsole": false,
      "debugServerArgs": "-f ${workspaceRoot}/openocd.cfg",
      "serverLaunchTimeout": 20000,
      "filterStderr": true,
      "filterStdout": false,
      "serverStarted": "Info\\ :\\ [\\w\\d\\.]*:\\ hardware",
      "setupCommands": [
        {
          "text": "cd ${workspaceRoot}"
        },
        {
          "text": "file build/game01_nucleof446re.elf",
          "description": "load file",
          "ignoreFailures": false
        },
        {
          "text": "target extended-remote localhost:3333",
          "description": "connect to target",
          "ignoreFailures": false
        },
        {
          "text": "monitor reset halt",
          "description": "perform a reset and halt the target",
          "ignoreFailures": false
        },
        {
          "text": "load",
          "description": "flash target",
          "ignoreFailures": false
        },
        {
          "text": "monitor reset init",
          "description": "perform a reset and init the target",
          "ignoreFailures": false
        },
        {
          "text": "set output-radix 16",
          "description": "set the default numeric base to 16",
          "ignoreFailures": false
        }
      ],
      "logging": {
        "moduleLoad": true,
        "trace": true,
        "engineLogging": true,
        "programOutput": true,
        "exceptions": true
      },
      "MIMode": "gdb",
      "miDebuggerPath": "/usr/bin/gdb-multiarch",
      "debugServerPath": "/usr/bin/openocd"
    },
    {
      "name": "C/C++ Runner: Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": [],
      "stopAtEntry": false,
      "externalConsole": false,
      "cwd": "/home/masoud/Desktop/embedded_project_for_learning/Core/Src",
      "program": "/home/masoud/Desktop/embedded_project_for_learning/Core/Src/build/Debug/outDebug",
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}